{
  "name": "sj",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "./node_modules/gulp/bin/gulp.js",
    "build": "browserify -t babelify --standalone sj src/main.js -o dist/sj.bundle.js",
    "build.min": "uglify -s dist/sj.bundle.js -o dist/sj.bundle.min.js",
    "test": "./node_modules/tape/bin/tape test/*.js",
    "browserTest": "browserify -t babelify test/*.js | testling"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  },
