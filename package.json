{
  "name": "sj",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "browserify -t babelify --standalone sj src/main.js -o dist/sj.bundle.js",
    "watch": "watchify -t babelify --standalone sj src/main.js -o dist/sj.bundle.js",
    "build.min": "uglify -s dist/sj.bundle.js -o dist/sj.bundle.min.js",
    "test": "./node_modules/tape/bin/tape test/*.js",
    "browserTest": "browserify -t babelify test/*.js | testling"
  },
  "browserify": {
    "transform": [ "babelify" ]
  },
  "testling": {
    "files": ["test.js", "test/test-sj-expression.js"]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "incremental-dom": "^0.4.1",
    "string.prototype.startswith": "^0.2.0",
    "webcomponents.js": "^0.7.22",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "tape": "^4.5.1"
  }
}
